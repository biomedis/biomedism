<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="500.0" minWidth="700.0" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.biomedis.biomedismair3.AppController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="%app.menu.file">
            <items>
                  <Menu mnemonicParsing="false" text="%app.menu.export">
                    <items>
                        <MenuItem fx:id="menuExportProfile" mnemonicParsing="false" onAction="#onExportProfile" text="%app.menu.export_profile" />
                      <MenuItem fx:id="menuExportUserBase" mnemonicParsing="false" onAction="#onExportUserBase" text="%app.menu.export_user_base" />
                        <MenuItem fx:id="menuExportTherapyComplex" mnemonicParsing="false" onAction="#onExportTherapyComplex" text="%app.menu.export_therapy_complex" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="%app.menu.import">
                    <items>
                        <MenuItem fx:id="menuImportProfile" mnemonicParsing="false" onAction="#onImportProfile" text="%app.menu.import_profile" />
                      <MenuItem fx:id="menuImportUserBase" mnemonicParsing="false" onAction="#onImportUserBase" text="%app.menu.import_user_base" />
                        <MenuItem fx:id="menuImportTherapyComplex" mnemonicParsing="false" onAction="#onImportTherapyComplex" text="%app.menu.import_therapy_complex" />
                        <MenuItem fx:id="menuImportProfileDir" mnemonicParsing="false" text="Импорт профиля из папки" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="%app.menu.options">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#onLangChoose" text="%app.menu.language_select" />
                    </items>
                  </Menu>
              <MenuItem fx:id="menuClose" mnemonicParsing="false" text="%app.close" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="%app.menu.service">
              <items>
                  <MenuItem fx:id="menuFindDevice" mnemonicParsing="false" onAction="#onFindDevice" text="%app.menu.find_device" />
                <MenuItem fx:id="menuClearFileSystemDevice" mnemonicParsing="false" onAction="#onClearDevice" text="%app.menu_filesystem_check" />
                  <MenuItem fx:id="menuDelGeneratedFiles" mnemonicParsing="false" onAction="#onRemoveProfileFiles" text="%app.menu.del_files" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="%app.menu.help">
            <items>
                  <MenuItem fx:id="menuManual" mnemonicParsing="false" onAction="#onHelp" text="%app.menu.manual" />
              <MenuItem fx:id="menuAbout" mnemonicParsing="false" onAction="#onAbout" text="%app.menu.about" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <VBox maxHeight="1.7976931348623157E308" spacing="8.0" BorderPane.alignment="CENTER">

         <children>
            <ComboBox fx:id="baseCombo" prefWidth="250.0" promptText="%ui.main.base_prompt" />
            <ComboBox fx:id="sectionCombo" prefWidth="250.0" />
            <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="5.0">
               <children>
                  <TextField fx:id="searchPatternField" promptText="%app.prompt_search" HBox.hgrow="ALWAYS" />
                  <Button fx:id="searchBtn" mnemonicParsing="false" text="%app.search" />
               </children>
            </HBox>
            <HBox fx:id="userActionPane" prefWidth="250.0" spacing="5.0">
               <children>
                  <Button fx:id="createUserBtn" mnemonicParsing="false" onAction="#onCreateUserBtn" text="%app.create" />
                  <Button fx:id="editUserBtn" mnemonicParsing="false" onAction="#onEditUserBtn" text="%app.edit" />
                  <Button fx:id="delUserBtn" mnemonicParsing="false" text="%app.delete" />
               </children>
            </HBox>
            <TreeView fx:id="sectionTree" maxHeight="1.7976931348623157E308" minHeight="100.0" prefHeight="200.0" prefWidth="250.0" VBox.vgrow="ALWAYS" />
            <TextArea fx:id="programInfo" maxHeight="150.0" minHeight="70.0" prefHeight="70.0" prefWidth="250.0" />
            <TextArea fx:id="programDescription" maxHeight="150.0" minHeight="50.0" prefHeight="50.0" prefWidth="250.0" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </left>
   <center>
      <VBox prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" prefHeight="120.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
               <children>
                  <ImageView fx:id="deviceIcon" fitHeight="100.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="8.0" right="8.0" />
                     </HBox.margin>
                  </ImageView>
                  <Group>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <children>
                        <ProgressBar fx:id="diskSpaceBar" layoutY="100.0" prefHeight="10.0" prefWidth="100.0" progress="0.0" rotate="270.0" translateY="35.0" />
                     </children>
                  </Group>
                  <Separator orientation="VERTICAL" prefHeight="200.0">
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin></Separator>
                  <StackPane HBox.hgrow="ALWAYS">
                     <children>
                        <VBox fx:id="progress2Pane" alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="100.0" spacing="8.0" visible="false">
                           <padding>
                              <Insets left="8.0" />
                           </padding>
                           <children>
                              <Label fx:id="textInfo" maxWidth="1.7976931348623157E308" text="Label">
                                 <VBox.margin>
                                    <Insets bottom="20.0" />
                                 </VBox.margin>
                              </Label>
                              <Label fx:id="textActionInfo" maxWidth="1.7976931348623157E308" text="Label" />
                              <ProgressBar fx:id="progressAction" maxWidth="400.0" prefWidth="200.0" progress="0.0" />
                           </children>
                        </VBox>
                        <VBox fx:id="progress1Pane" alignment="CENTER" maxWidth="1.7976931348623157E308" visible="false">
                           <children>
                              <ProgressIndicator fx:id="progressIndicator" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                 </VBox.margin>
                                 <effect>
                                    <Reflection bottomOpacity="0.13" fraction="0.32" topOpacity="0.2" />
                                 </effect>
                              </ProgressIndicator>
                              <Label fx:id="progressIndicatorLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label">
                                 <VBox.margin>
                                    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <VBox fx:id="progress3Pane" alignment="CENTER" maxWidth="1.7976931348623157E308" visible="false" StackPane.alignment="CENTER">
                           <children>
                              <Label fx:id="messageText" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="Label" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <Separator prefWidth="200.0">
               <VBox.margin>
                  <Insets left="18.0" right="18.0" />
               </VBox.margin>
            </Separator>
            <HBox maxWidth="1.7976931348623157E308" prefWidth="200.0" spacing="8.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="btnCreateProfile" mnemonicParsing="false" onAction="#onCreateProfile" text="%ui.main.create_profile" />
                  <Button fx:id="btnDeleteProfile" mnemonicParsing="false" onAction="#onRemoveProfile" text="%ui.main.remove_profile" />
                  <Button fx:id="btnGenerate" mnemonicParsing="false" onAction="#onGenerate" text="%app.file_generation" />
                  <Button fx:id="btnUpload" mnemonicParsing="false" onAction="#onUploadProfile" text="%app.uppload" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
            <TableView fx:id="tableProfile" maxHeight="100.0" maxWidth="1.7976931348623157E308" minHeight="50.0" prefHeight="60.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" />
               </VBox.margin>
            </TableView>
            <HBox maxWidth="1.7976931348623157E308" prefWidth="200.0" spacing="8.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="btnCreateTherapy" mnemonicParsing="false" onAction="#onCreateComplex" text="%ui.main.create_complex" />
                  <Button fx:id="btnDeleteTherapy" mnemonicParsing="false" onAction="#onRemoveComplex" text="%ui.main.remove_complex" />
                  <HBox fx:id="spinnerPan" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="8.0" right="8.0" />
                     </HBox.margin>
                     <children>
                        <Label alignment="CENTER" text="%ui.time_to_freq">
                           <HBox.margin>
                              <Insets left="8.0" right="8.0" />
                           </HBox.margin>
                        </Label>
                        <Spinner fx:id="timeToFreqSpinner" maxWidth="80.0" />
                        <VBox fx:id="spinnerBtnPan" spacing="3.0">
                           <children>
                              <Button fx:id="btnOkSpinner" contentDisplay="CENTER" ellipsisString="" maxHeight="16.0" maxWidth="16.0" minHeight="16.0" minWidth="16.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="16.0" />
                              <Button fx:id="btnCancelSpinner" maxHeight="16.0" maxWidth="16.0" minHeight="16.0" minWidth="16.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="16.0" />
                           </children>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
            <TableView fx:id="tableComplex" minHeight="50.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" />
               </VBox.margin>
            </TableView>
            <HBox maxWidth="1.7976931348623157E308" prefWidth="200.0" spacing="8.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="btnDeleteProgram" mnemonicParsing="false" onAction="#onRemoveProgram" text="%ui.main.remove_program" />
                  <Button fx:id="btnUpProgram" mnemonicParsing="false" onAction="#onUpProgram" text="%app.up" />
                  <Button fx:id="btnDownProgram" mnemonicParsing="false" onAction="#onDownProgram" text="%app.down" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
            <TableView fx:id="tableProgram" minHeight="50.0" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </center>
</BorderPane>
