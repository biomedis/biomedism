<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.biomedis.biomedismair3.social.login.InputCredentialController">
   <children>
      <Label text="Email:" />
      <TextField fx:id="email" minWidth="250.0">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </TextField>
      <Label text="Password:" />
      <TextField fx:id="password" minWidth="250.0">
         <VBox.margin>
            <Insets bottom="8.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="logIn" onAction="#onLoginAction" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Войти">
         <styleClass>
            <String fx:value="GenericBtn" />
         </styleClass>
      </Button>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
