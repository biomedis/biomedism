<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" prefHeight="170.0" xmlns="http://javafx.com/javafx/11.0.1"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="ru.biomedis.biomedismair3.social.account.ChangeEmailController"
  alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
  spacing="8.0" visible="false">
  <children>
    <Label text="Введите проверочный код, отправленный на новый email:"/>
    <TextField fx:id="inputCode"/>
    <Button fx:id="sendCode" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
      onAction="#onChangeEmailAction" text="Изменить email"/>
  </children>
  <padding>
    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
  </padding>
</VBox>

