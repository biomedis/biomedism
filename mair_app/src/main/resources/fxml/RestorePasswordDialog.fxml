<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Hyperlink?>
<VBox fx:id="root" prefHeight="170.0" xmlns="http://javafx.com/javafx/11.0.1"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.biomedis.biomedismair3.social.login.RestorePasswordController">
   <children>
      <Label text="Проверочный код, отправленный на email:"/>
      <TextField fx:id="codeInput"/>
      <Label text="Email:"/>
      <TextField fx:id="emailInput"/>
      <Label text="Новый пароль:"/>
      <TextField fx:id="passwordInput"/>
      <Button fx:id="updatePasswordBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
        onAction="#onNewPassword" text="Изменить пароль"/>
      <Hyperlink fx:id="resendLink" maxWidth="1.7976931348623157E308" text="Отправить повторно код" onAction="#onResend"/>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
   </padding>
</VBox>

