<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListCell?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root maxWidth="1.7976931348623157E308" type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <graphic>
    <VBox fx:id="rootBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="7.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <children>
            <HBox maxWidth="1.7976931348623157E308">
               <children>
                  <Label fx:id="date" text="20.02.2020" HBox.hgrow="ALWAYS" />
                  <HBox maxWidth="1.7976931348623157E308">
                     <children>
                        <Button fx:id="editBtn" mnemonicParsing="false">
                           <styleClass>
                              <String fx:value="GenericBtn" />
                              <String fx:value="EditSmallBtn" />
                           </styleClass>
                        </Button>
                        <Button fx:id="deleteBtn" mnemonicParsing="false">
                           <styleClass>
                              <String fx:value="GenericBtn" />
                              <String fx:value="deleteSmallBtn" />
                           </styleClass>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <Label fx:id="title" text="Название" />
            <HBox maxWidth="1.7976931348623157E308" spacing="8.0">
               <children>
                  <ImageView fx:id="image" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
                  <Label fx:id="description" maxHeight="1.7976931348623157E308" text="Label" wrapText="true" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox maxWidth="1.7976931348623157E308">
               <children>
                  <Hyperlink fx:id="fullContent" text="Подробнее" />
               </children>
            </HBox>
         </children>
    </VBox>
  </graphic>
</fx:root>
